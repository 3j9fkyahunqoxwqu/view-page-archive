{
  "name": "view-page-archive",
  "version": "1.1.2",
  "author": "Armin Sebastian",
  "repository": "https://github.com/dessant/view-page-archive",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=development gulp build",
    "build:opera": "NODE_ENV=development TARGET_ENV=opera gulp build",
    "build:chrome": "NODE_ENV=development TARGET_ENV=chrome gulp build",
    "build:prod": "NODE_ENV=production gulp build",
    "build:prod:opera": "NODE_ENV=production TARGET_ENV=opera gulp build",
    "build:prod:chrome": "NODE_ENV=production TARGET_ENV=chrome gulp build",
    "build:ext:prod": "NODE_ENV=production SYS_DEPS=false gulp build && web-ext build -s dist/ -a artifacts/",
    "build:ext:prod:opera": "NODE_ENV=production TARGET_ENV=opera SYS_DEPS=false gulp build && web-ext build -s dist/ -a artifacts/",
    "build:ext:prod:chrome": "NODE_ENV=production TARGET_ENV=chrome SYS_DEPS=false gulp build && web-ext build -s dist/ -a artifacts/",
    "start": "web-ext run --bc --url 'about:debugging' --url 'about:addons' -s dist/",
    "exec": "yarn run build && yarn run start",
    "exec:prod": "yarn run build:prod && yarn run start",
    "inspect": "webpack --profile --json > report.json; webpack-bundle-analyzer report.json --mode static && sleep 6 && rm report.{json,html}",
    "update": "ncu --upgrade --upgradeAll && yarn upgrade --latest",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version && git push --follow-tags origin master"
  },
  "browserslist": [
    "Chrome >= 55",
    "Firefox >= 52",
    "Opera >= 42"
  ],
  "dependencies": {
    "@material/button": "^0.6.0",
    "@material/checkbox": "^0.4.6",
    "@material/form-field": "^0.2.15",
    "@material/list": "^0.2.18",
    "@material/ripple": "^0.8.6",
    "@material/select": "^0.3.16",
    "@material/switch": "^0.1.13",
    "@material/typography": "^0.3.0",
    "lodash": "^4.17.4",
    "vue": "^2.4.4",
    "vuedraggable": "^2.14.1",
    "webextension-polyfill": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.6.0",
    "babel-preset-minify": "^0.2.0",
    "commander": "^2.11.0",
    "conventional-changelog-cli": "^1.3.3",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs-extra": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.3.0",
    "gulp-jsbeautifier": "^2.1.1",
    "gulp-merge-json": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rsvg": "^1.0.2",
    "gulp-sequence": "^0.4.6",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.5.3",
    "npm-check-updates": "^2.12.1",
    "recursive-readdir": "^2.2.1",
    "sass-loader": "^6.0.6",
    "shortid": "^2.2.8",
    "standard-version": "^4.2.0",
    "svg2png": "^4.1.1",
    "typeface-roboto": "^0.0.35",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.4",
    "web-ext": "^2.0.0",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "private": true
}
